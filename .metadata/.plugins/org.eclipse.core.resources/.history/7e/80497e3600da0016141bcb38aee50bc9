#include <iostream>
using namespace std;

class A
{
public:
	A(int);
	void f();
	void print();
	~A();
private:
	int i;
};

A::A(int v):i(v)
{
	cout << "A::A(), i = " << this->i << endl;
}

A::~A()
{
	cout << "A::~A(), i = " << this->i << endl;
}

int main()
{
	A *p = NULL;
	p = new A[3];

	delete p;

	return 0;
}


