#include <iostream>
using namespace std;

class A
{
public:
	A(int);
	void f();
	void print();
private:
	int i;
};

A::A(int i)
{
	this->i = i;
	cout << "A::A(), i = " << this->i << endl;
}

int main()
{
	A *p;
	p = new A[10];

	delete []p;

	return 0;
}


