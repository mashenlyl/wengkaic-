#include <iostream>
using namespace std;

class A
{
public:
	int i;
	void f();
};

void A::f()
{
	i = 20;
	cout << i << endl;
}

int main()
{
	A a;
	A b;
	cout << "a.f() " << a.f() << endl;
	cout << "a.f() " << b.f() << endl;

	return 0;
}


