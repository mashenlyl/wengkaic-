#include <iostream>
using namespace std;

class A
{
private:
	int a;
public:
	A(int a){ this->a = a; };
	void f(A *p){ p->a = 10;};
	int print(){ return this->a; }
};

int main()
{
	A a(100);
	A b(30);
	a.f(&b);
	cout << "a.a" << a.print() << endl;
	cout << "b.a" << b.print() << endl;

	return 0;
}


