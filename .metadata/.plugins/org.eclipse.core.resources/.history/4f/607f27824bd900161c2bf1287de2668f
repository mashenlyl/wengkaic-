#include <iostream>
using namespace std;

class Point3d
{
public:
	// 定义的同时对类进行初始化
	Point3d(float x, float y, float z);
	print();

//这个类有三个数据
private:
	float x;
	float y;
	float z;
};

void print(void)
{
	cout << "x = " << x << "y = " << y << "z = " << z <<endl;
}

int main()
{

}




