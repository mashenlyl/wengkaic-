#include <stdio.h>

struct Point3d
{
	float x;
	float y;
	float z;
}Pointed;

void print(Pointed *a)
{
	prinf("x = %f\n", a->x);
	prinf("x = %f\n", a->y);
	prinf("x = %f\n", a->z);
}

int main(void)
{
	Pointed a;
	a.x = 10;
	a.y = 20;
	a.z = 30;
	print(&a);
	return 0;
}



