#include <iostream>
using namespace std;

class A
{
public:
	A():i(2){};
	~A(){};
	void print() { cout << "A::print " << i << endl; };
protected:
	void set(int a) { i=a; };
private:
	int i;
};

class B: public A
{

};

int main()
{
	B c;
	//c.set(23);
	c.print();

	return 0;
}


