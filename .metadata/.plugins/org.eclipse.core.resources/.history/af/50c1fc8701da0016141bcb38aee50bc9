#include <iostream>
using namespace std;

class A
{
public:
	A(int);
	void set(int);
	~A();
private:
	int i;
};

A::A()
{
	cout << "A::A(), i = " << endl;
}

A::~A()
{
	cout << "A::~A(), i = " << this->i << endl;
}

void A:set(int i)
{
	this->i = i;
}

int main()
{
	A *p = NULL;


	delete p;

	return 0;
}


